: [],\r\n\t\t\t\tloading: false,\r\n\t\t\t\tpagination: {\r\n\t\t\t\t\tdefaultPageSize: 20,\r\n\t\t\t\t\tpageSize: 20,\r\n\t\t\t\t\tcurrent: 1,\r\n\t\t\t\t\ttotal: 0,\r\n\t\t\t\t\thideOnSinglePage: true\r\n\t\t\t\t},\r\n\t\t\t\tresVisible: false,\r\n\t\t\t\tresources: [],\r\n\t\t\t\troleId: '',\r\n\t\t\t\tscrollx: '',\r\n\t\t\t\tscrolly: ''\r\n\t\t\t};\r\n\t\t},\r\n\t\tcreated() {\r\n\t\t\tthis.getData()\r\n\t\t},\r\n\t\tmounted() {},\r\n\t\tmethods: {\r\n\t\t\tgetData(data) {\r\n\t\t\t\tthis.loading = true\r\n\t\t\t\tgetRolesList({\r\n\t\t\t\t\tPageIndex: 0,\r\n\t\t\t\t\tPageSize: 25\r\n\t\t\t\t}).then(res => {\r\n\t\t\t\t\tif (res.status === '0') {\r\n\t\t\t\t\t\tthis.data = res.result.list\r\n\t\t\t\t\t} else {\r\n\t\t\t\t\t\tconsole.log(res.message)\r\n\t\t\t\t\t}\r\n\t\t\t\t\tthis.loading = false\r\n\t\t\t\t})\r\n\t\t\t},\r\n\t\t\t// 提交\r\n\t\t\thandleOk(e) {\r\n\t\t\t\te.preventDefault();\r\n\t\t\t\tthis.form.validateFields((err, values) => {\r\n\t\t\t\t\tif (!err) {\r\n\t\t\t\t\t\tconsole.log(values)\r\n\t\t\t\t\t\tthis.confirmLoading = true\r\n\t\t\t\t\t\tif (this.modalTitle == '新增') {\r\n\t\t\t\t\t\t\tthis.added(values)\r\n\t\t\t\t\t\t} else {\r\n\t\t\t\t\t\t\tthis.edit(values)\r\n\t\t\t\t\t\t}\r\n\t\t\t\t\t}\r\n\t\t\t\t});\r\n\t\t\t},\r\n\t\t\thandleCancel() {\r\n\t\t\t\tthis.reduction()\r\n\t\t\t\tthis.modalShow = false;\r\n\t\t\t},\r\n\t\t\t// 新增\r\n\t\t\ttoAdded() {\r\n\t\t\t\tthis.modalTitle = '新增'\r\n\t\t\t\tthis.modalShow = true;\r\n\t\t\t},\r\n\t\t\tadded(data) {\r\n\t\t\t\taddRole(data).then(res => {\r\n\t\t\t\t\tconsole.log(res)\r\n\t\t\t\t\tif (res.status === '0') {\r\n\t\t\t\t\t\tthis.modalShow = false\r\n\t\t\t\t\t\tthis.reduction()\r\n\t\t\t\t\t\tthis.$message.success('添加成功', 3, this.getData());\r\n\t\t\t\t\t} else {\r\n\t\t\t\t\t\tconsole.log(res.message)\r\n\t\t\t\t\t}\r\n\t\t\t\t\tthis.confirmLoading = false\r\n\t\t\t\t})\r\n\t\t\t},\r\n\t\t\t// 修改\r\n\t\t\ttoEdit(data) {\r\n\t\t\t\tconsole.log(data)\r\n\t\t\t\tthis.modalTitle = '修改'\r\n\t\t\t\tthis.modalShow = true;\r\n\t\t\t\tthis.form.setFieldsValue({\r\n\t\t\t\t\troleName: data.roleName,\r\n\t\t\t\t\tremarks: data.remarks,\r\n\t\t\t\t\tid: data.id\r\n\t\t\t\t});\r\n\t\t\t},\r\n\t\t\tedit(data) {\r\n\t\t\t\tputRole(data).then(res => {\r\n\t\t\t\t\tconsole.log(res)\r\n\t\t\t\t\tif (res.status === '0') {\r\n\t\t\t\t\t\tthis.modalShow = false\r\n\t\t\t\t\t\tthis.reduction()\r\n\t\t\t\t\t\tthis.$message.success('修改成功', 3, this.getData());\r\n\t\t\t\t\t} else {\r\n\t\t\t\t\t\tconsole.log(res.message)\r\n\t\t\t\t\t}\r\n\t\t\t\t\tthis.confirmLoading = false\r\n\t\t\t\t})\r\n\t\t\t},\r\n\t\t\t// 初始化form\r\n\t\t\treduction() {\r\n\t\t\t\tthis.form.setFieldsValue({\r\n\t\t\t\t\troleName: '',\r\n\t\t\t\t\tremarks: ''\r\n\t\t\t\t});\r\n\t\t\t},\r\n\t\t\tplan(id) {\r\n\t\t\t\tconsole.log(id)\r\n\t\t\t\tthis.roleId = id\r\n\t\t\t\tgetRoleResources({\r\n\t\t\t\t\troleId: id\r\n\t\t\t\t}).then(res => {\r\n\t\t\t\t\tif (res.status === '0') {\r\n\t\t\t\t\t\tconsole.log(res.result)\r\n\t\t\t\t\t\tthis.resources = res.result\r\n\t\t\t\t\t\tthis.resVisible = true\r\n\t\t\t\t\t} else {\r\n\t\t\t\t\t\tconsole.log(res.message)\r\n\t\t\t\t\t}\r\n\t\t\t\t})\r\n\t\t\t},\r\n\t\t\thandleChange(e, id, i) {\r\n\t\t\t\tthis.resources[i].isChoose = e.target.checked ? 1 : 0\r\n\t\t\t\tconst data = {\r\n\t\t\t\t\troleID: this.roleId,\r\n\t\t\t\t\tresourceID: id,\r\n\t\t\t\t\tisChoose: this.resources[i].isChoose\r\n\t\t\t\t}\r\n\t\t\t\tputRoleResources(data).then(res => {\r\n\t\t\t\t\tif (res.status === '0') {\r\n\t\t\t\t\t\tconsole.log(res)\r\n\t\t\t\t\t} else {\r\n\t\t\t\t\t\tconsole.log(res.message)\r\n\t\t\t\t\t\tthis.resources[i].isChoose = e.target.checked ? 0 : 1\r\n\t\t\t\t\t}\r\n\t\t\t\t})\r\n\t\t\t},\r\n\t\t\ttableHeight(data) {\r\n\t\t\t\tthis.scrolly = data - 75\r\n\t\t\t}\r\n\t\t}\r\n\t})\r\n</script>\r\n\r\n<style scoped>\r\n</style>\n"],"sourceRoot":"src/page/limit"}]}